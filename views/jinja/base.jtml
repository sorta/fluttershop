<!DOCTYPE html>
<html>

    <head>
        {% block head %}

            <!-- jQuery(ui) -->
            <link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.23/themes/sunny/jquery-ui.css" type="text/css" />

            <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
            <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8.23/jquery-ui.min.js"></script>

            <!-- bootstrap -->
            <link rel="stylesheet/less" href="/static/bootstrap/source/less/bootstrap.less">
            <link rel="stylesheet/less" href="/static/bootstrap/source/less/responsive.less">
            <!--
            <link rel="stylesheet" href="/static/bootstrap/css/bootstrap.css" type="text/css" />
            <link rel="stylesheet" href="/static/bootstrap/css/bootstrap-responsive.css" type="text/css" />
            -->

            <script language="javascript" src="/static/bootstrap/compiled/js/bootstrap.min.js"></script>


            {% if logged_in %}
                <!-- redactor -->
                <link rel="stylesheet" href="/static/js/redactor/css/redactor.css" />

                <script src="/static/js/redactor/redactor.js"></script>
            {% endif %}

            <!-- FlutterShop -->
            <!-- <link rel="stylesheet" href="/static/css/fshop.css" type="text/css" /> -->
            <link rel="stylesheet/less" href="/static/css/fshopLESS.less">
            {% if logged_in %}
                <script language="javascript" src="/static/js/fshopa.js"></script>
            {% endif %}

            <!-- less -->
            <script src="/static/js/less-1.3.0.min.js" type="text/javascript"></script>

            <!-- meta -->
            <title>{% block title %}{{ page_title }}{% endblock %}</title>
            <meta name="description" content="{% block description %}{{ page_desc }}{% endblock %}" />
            <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {% endblock %}
    </head>
    <body>
        {% block body %}
            {% block modals %}
                {% include 'jinja/modals.jtml' %}
            {% endblock %}

            {% block page %}

                {% block nav %}
                    {% include 'jinja/nav.jtml' %}
                {% endblock %}

                {% block vessel %}

                    {% block gutter_left %}

                    {% endblock %}

                    {% block spire %}

                    {% endblock %}

                    {% block gutter_right %}

                    {% endblock %}

                {% endblock %}

            {% endblock %}
        {% endblock %}
    </body>

</html>
