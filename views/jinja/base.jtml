<!DOCTYPE html>
<html>

    <head>
        {% block head %}
            <link rel="stylesheet" href="/static/bootstrap/css/bootstrap.css" type="text/css" />
            <link rel="stylesheet" href="/static/bootstrap/css/bootstrap-responsive.css" type="text/css" />
            <link rel="stylesheet" href="/static/css/fshop.css" type="text/css" />

            <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
            <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8.23/jquery-ui.min.js"></script>
            <script language="javascript" src="/static/bootstrap/js/bootstrap.js"></script>
            <script src="/static/js/less-1.3.0.min.js" type="text/javascript"></script>

            {% if logged_in %}
                <link rel="stylesheet" href="/static/js/redactor/css/redactor.css" />

                <script src="/static/js/redactor/redactor.js"></script>
                <script language="javascript" src="/static/js/fshopa.js"></script>
            {% endif %}
            <title>{% block title %}{{ page_title }}{% endblock %}</title>
            <meta name="description" content="{% block description %}{{ page_desc }}{% endblock %}" />
            <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {% endblock %}
    </head>
    <body>
        {% block body %}
            {% block modals %}
                {% include 'jinja/modals.jtml' %}
            {% endblock %}

            {% block page %}

                {% block nav %}
                    {% include 'jinja/nav.jtml' %}
                {% endblock %}

                {% block vessel %}

                    {% block gutter_left %}

                    {% endblock %}

                    {% block spire %}

                    {% endblock %}

                    {% block gutter_right %}

                    {% endblock %}

                {% endblock %}

            {% endblock %}
        {% endblock %}
    </body>

</html>
